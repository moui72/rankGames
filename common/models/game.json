{
  "name": "Game",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "gameId": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "owners": {
      "type": "hasMany",
      "model": "Gamer",
      "foreignKey": "GamerIds",
      "through": "Library"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
