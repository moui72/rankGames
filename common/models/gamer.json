{
  "name": "Gamer",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "default": "Gamer"
    },
    "joined": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "bggUser": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "libraries": {
      "type": "hasMany",
      "model": "Library",
      "foreignKey": "ownerId"
    },
    "lists": {
      "type": "hasMany",
      "model": "list",
      "foreignKey": "ownerId",
      "through": "library"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
